<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="crm_lead_view_form" model="ir.ui.view">
        <field name="name">crm.lead.sd_opportunity_effort_level.form</field>
        <field name="model">crm.lead</field>
        <field name="inherit_id" ref="crm.crm_lead_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='company_id']" position="after" >
                <field name="floe_total" widget="float_time" />
                <field name="floe_c" widget="float_time" />
                <field name="floe_d" widget="float_time" />
                <field name="floe_q" widget="float_time" />
            </xpath>
        </field>
    </record>
</odoo>

