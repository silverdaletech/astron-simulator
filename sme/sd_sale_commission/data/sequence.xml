<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="seq_sale_commissions" model="ir.sequence">
        <field name="name">Sale Commissions</field>
        <field name="code">seq.sale.commissions</field>
        <field name="prefix">SC/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_commissions_settlement" model="ir.sequence">
        <field name="name">Commissions Settlement</field>
        <field name="code">seq.commissions.settlement</field>
        <field name="prefix">SCS/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
