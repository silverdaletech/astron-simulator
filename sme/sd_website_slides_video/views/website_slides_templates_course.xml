<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <template id="sd_website_slides_video_inherit_slide_icon" inherit_id="website_slides.slide_icon">
            <xpath expr="//i[last()]" position="after">
                <i t-if="slide.slide_type == 'doc_video'" t-att-class="'fa fa-play-circle %s' % icon_class"/>
            </xpath>
        </template>

    </data>
</odoo>